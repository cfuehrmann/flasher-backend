    "@types/uuid": "^3.4.4",
    "cors": "^2.8.5",
    "date-fns": "^1.30.1",
    "express": "^4.17.1",
    "express-graphql": "^0.8.0",
    "graphql": "^14.3.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "chokidar-cli": "^1.2.2",
    "husky": "^2.4.0",
    "mocha": "^6.1.4",
    "prettier": "^1.17.1",
    "pretty-quick": "^1.11.0",
    "tslint": "^5.17.0",
